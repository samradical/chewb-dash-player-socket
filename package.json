{
  "name": "@samelie/dash-player-youtube-socket",
  "description": "Retreive the sidx of a youtube video from the id.",
  "homepage": "https://github.com/samradical/dash-player-socket",
  "author": "Sam Elie <samradelie@gmail.com>",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/samradical/dash-player-socket.git",
    "web": "https://github.com/samradical/dash-player-socket/"
  },
  "main": "index.js",
  "scripts": {
    "start": "budo index.js:bundle.js  --host 0.0.0.0 --live | garnish -t [babelify --stage 1 ]",
    "dev": "budo example.js:bundle.js  --host 0.0.0.0 --live | garnish -t [babelify --stage 1 ]",
    "build": "browserify index.js > bundle.js"
  },
  "dependencies": {
    "@samelie/dash-player-errortypes": "git://github.com/samradical/dash-player-errortypes.git",
    "@samelie/dash-player-voutils": "git://github.com/samradical/dash-player-voutils.git",
    "bluebird": "^3.4.6",
    "lodash": "^4.16.4",
    "signals": "^1.0.0",
    "socket.io-client": "^1.5.0",
    "xhr-request": "^1.0.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^11.1.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-1"
          ]
        }
      ]
    ]
  },
  "keywords": [
    "youtube",
    "dash",
    "sidx",
    "mpd"
  ],
  "bugs": {
    "url": "https://github.com/samradical/dash-player-socket/issues"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/samradical/dash-player-socket/raw/master/LICENSE"
    }
  ]
